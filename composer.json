{
    "name": "pijler/laravel-modules",
    "type": "library",
    "description": "Simple package for organizing Laravel projects using modules.",
    "keywords": [
        "laravel",
        "modules",
        "modular",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jo√£o Pedro Lopes",
            "email": "joaopalopes24@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0",
        "orchestra/testbench": "^10.6",
        "laravel/pint": "^1.25",
        "inertiajs/inertia-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable"
}